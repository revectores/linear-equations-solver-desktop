eq_test: eq_test.o eq.o utils.o
	g++ -o eq_test eq_test.o eq.o utils.o -std=c++2a

eq_test.o: eq_test.cpp
	g++ -o eq_test.o -c eq_test.cpp -std=c++2a

eq.o: eq.cpp
	g++ -o eq.o -c eq.cpp -std=c++2a

utils.o: utils.cpp
	g++ -o utils.o -c utils.cpp -std=c++2a

clean:
	rm eq.o eq_test.o utils.o eq_test
